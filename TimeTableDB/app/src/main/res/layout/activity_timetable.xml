                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button6"
                                android:layout_width="fill_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="380dp"
                                android:background="@null"
                                android:text="JO011"
                                android:textSize="10sp"
                                android:visibility="gone" />
                        </RelativeLayout>

                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>

</LinearLayout>