                        android:textColor="#ffbb00" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayout3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="14"
                    android:padding="0dp" >

                    <LinearLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:padding="0dp" >

                        <RelativeLayout
                            android:id="@+id/relativeLayoutMonday"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2" >

                            <View
                                android:layout_width="1dp"
                                android:layout_height="fill_parent"
                                android:layout_alignParentRight="true"
                                android:background="#ffbb00" />

                            <View
                                android:layout_width="fill_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="180dp"
                                android:background="#9ACC61"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button1"
                                android:layout_width="fill_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="180dp"
                                android:background="@null"
                                android:text="JO088"
                                android:textSize="10sp"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayoutTueDay"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2" >

                            <View
                                android:layout_width="1dp"
                                android:layout_height="fill_parent"
                                android:layout_alignParentRight="true"
                                android:background="#ffbb00" />

                            <View
                                android:layout_width="fill_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="280dp"
                                android:background="#9ACC61"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button2"
                                android:layout_width="fill_parent"
                                android:layout_height="180dp"
                                android:layout_marginTop="280dp"
                                android:background="@null"
                                android:text="JO022"
                                android:textSize="10sp"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayoutWedDay"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2" >

                            <View
                                android:layout_width="1dp"
                                android:layout_height="fill_parent"
                                android:layout_alignParentRight="true"
                                android:background="#ffbb00" />

                            <View
                                android:layout_width="fill_parent"
                                android:layout_height="60dp"
                                android:layout_marginTop="40dp"
                                android:background="#9ACC61"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button3"
                                android:layout_width="fill_parent"
                                android:layout_height="60dp"
                                android:layout_marginTop="40dp"
                                android:background="@null"
                                android:text="JO002"
                                android:textSize="10sp"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayoutThuDay"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2" >

                            <View
                                android:layout_width="1dp"
                                android:layout_height="fill_parent"
                                android:layout_alignParentRight="true"
                                android:background="#ffbb00" />

                            <View
                                android:layout_width="fill_parent"
                                android:layout_height="90dp"
                                android:layout_marginTop="60dp"
                                android:background="#9ACC61"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button4"
                                android:layout_width="fill_parent"
                                android:layout_height="90dp"
                                android:layout_marginTop="60dp"
                                android:background="@null"
                                android:text="JO003"
                                android:textSize="10sp"
                                android:visibility="gone" />

                            <View
                                android:layout_width="fill_parent"
                                android:layout_height="120dp"
                                android:layout_marginTop="340dp"
                                android:background="#9ACC61"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/button5"
                                android:layout_width="fill_parent"
                                android:layout_height="120dp"
                                android:layout_marginTop="340dp"
                                android:background="@null"
                                android:text="JO021"
                                android:textSize="10sp"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayoutFriDay"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2" >

                            <View
